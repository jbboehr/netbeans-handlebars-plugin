<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "https://netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>

    <folder name="Editors">
        <folder name="text">
            <folder name="x-handlebars">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="net.johnboehr.netbeans.handlebars.resources.Bundle"/>
                <file name="net-johnboehr-netbeans-handlebars-parser-HandlebarsParserFactory.instance"/>
                <file name="net-johnboehr-netbeans-handlebars-parser-HandlebarsSyntaxErrorHighlightingTaskFactory.instance"/>
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="FontAndColors.xml" url="FontAndColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="net.johnboehr.netbeans.handlebars.resources.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
                <file name="language.instance">
                    <attr name="instanceCreate" methodvalue="net.johnboehr.netbeans.handlebars.lexer.HandlebarsTokenId.language"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.api.lexer.Language"/>
                </file>
            </folder>
        </folder>
    </folder>

    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="x-handlebars" url="HandlebarsTemplate.handlebars"/>
            </folder>
        </folder>
    </folder>

</filesystem>